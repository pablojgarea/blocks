<?xml version="1.0"?>
<schema version="0.3">

	<table name="btGallery">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="fsID" type="I"><unsigned /></field>		
		<field name="playback" type="C" size="50"></field>	
		<field name="duration" type="I"><unsigned /></field>
		<field name="fadeDuration" type="I"><unsigned /></field>
		<field name="thumbnailWidth" type="I"><unsigned /></field>
		<field name="thumbnailHeight" type="I"><unsigned /></field>
		<field name="thumbnailPerRow" type="I"><unsigned /></field>
		<field name="maxHeight" type="I"><unsigned /></field>
		<field name="maxThumbs" type="I"><unsigned /></field>
		<field name="shownAttributes" type="C" size="255"></field>
		<field name="thumbnailAttributeKeyID" type="I"><default value="0" /><unsigned /></field>
	</table>
	
	<table name="btGalleryImg">
		<field name="galleryImgId" type="I">
			<key />
			<autoincrement />
			<unsigned />
		</field>
		<field name="bID" type="I"><unsigned /></field>
		<field name="fID" type="I"><unsigned /></field>		
		<field name="url" type="C" size="255"></field>
		<field name="caption" type="X"/>
		<field name="duration" type="I"><unsigned /></field>
		<field name="fadeDuration" type="I"><unsigned /></field>
		<field name="groupSet" type="I"><unsigned /></field>
		<field name="position" type="I"><unsigned /></field>	
		<field name="imgHeight" type="I"><unsigned /></field>
		<field name="shownAttributes" type="C" size="255"></field>							
	</table>
	
</schema>
